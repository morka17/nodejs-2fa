    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "otplib": "^12.0.1",
    "sqlite": "^5.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
